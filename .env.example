# Database Configuration
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key

# External Order Service Configuration
# Set to 'true' to enable external order service integration
NEXT_PUBLIC_EXTERNAL_ORDER_ENABLED=false

# External order service URL (only used if enabled)
NEXT_PUBLIC_EXTERNAL_ORDER_URL=https://api.example.com/place_order

# Request timeout in milliseconds (default: 5000)
NEXT_PUBLIC_EXTERNAL_ORDER_TIMEOUT=5000

# Number of retry attempts for failed requests (default: 2)
NEXT_PUBLIC_EXTERNAL_ORDER_RETRIES=2

# Development mode
NODE_ENV=development